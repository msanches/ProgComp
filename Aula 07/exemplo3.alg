algoritmo exemplo3
	inicio
		real nota1, nota2, media
		inteiro contador
		contador = 1
		enquanto(contador < = 10){
			escreva("Digite digite a primeira nota:")
			leia(nota1)
			escreva("Digite digite a segunda nota:")
			leia(nota2)			
			media = (nota1 = nota2)/2
			escreva("A média é:" + media)
			contador=contador +1
		}
	fim
	